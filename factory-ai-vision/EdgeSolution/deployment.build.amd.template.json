{
  "$schema-template": "2.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "intelcustomvision": {
                "username": "$CONTAINER-REGISTRY-USERNAME",
                "password": "$CONTAINER-REGISTRY-PASSWORD",
                "address": "$CONTAINER-REGISTRY-NAME"
              }
            }
          }
        },
        "systemModules": {},
        "modules": {
          "nginxmodule": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/NginxModule",
              "createOptions": "{}"
            }
          },
          "webmodule": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/WebModule",
              "createOptions": "{}"
            }
          },
          "inferencemodule": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/InferenceModule",
              "createOptions": "{}"
            }
          },
          "PredictModuleCPU": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/PredictModule",
              "createOptions": "{}"
            }
          },
          "PredictModuleGPU": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/PredictModule",
              "createOptions": "{}"
            }
          },
          "PredictModuleVPU": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/PredictModule",
              "createOptions": "{}"
            }
          },
          "rtspsim": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/RtspSimModule",
              "createOptions": "{}"
            }
          },
          "uploadmodule": {
            "type": "docker",
            "settings": {
              "image": "lindamthomas/intelligentedge/UploadModule",
              "createOptions": "{}"
            }
          }
        }
      }
    }
  }
}
